    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {"PYTHONPATH": "${cwd}"
            }
        },
        {
            "name": "Python jupyter",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal",
            "cwd": "${workspaceFolder}",
            "env": {"PYTHONPATH": "${cwd}"
            }
        }
    ]
}